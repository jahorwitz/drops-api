{
  "name": "drops-backend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "dev": "keystone dev",
    "start": "keystone start",
    "build": "keystone build && keystone prisma migrate deploy",
    "generate": "keystone prisma migrate dev",
    "deploy": "keystone build && keystone prisma migrate deploy",
    "postinstall": "keystone build --no-ui --frozen",
    "test": "jest --passWithNoTests",
    "lint": "npx prettier --check --ignore-unknown \"./**/*.ts\"",
    "lint:fix": "npx prettier --write --ignore-unknown \"./**/*.ts\"",
    "clean": "rm -rf dist",
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.ts$"
  },
  "dependencies": {
    "@keystone-6/auth": "^7.0.0",
    "@keystone-6/core": "^5.2.0",
    "@keystone-6/fields-document": "^7.0.0",
    "@prisma/debug": "^5.3.1",
    "@prisma/generator-helper": "^5.3.1",
    "dotenv": "^16.3.1",
    "fs-extra": "^11.2.0",
    "geojson": "^0.5.0",
    "keystone": "^0.3.22",
    "lodash": "^4.17.21",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "@types/babel__core": "^7.20.5",
    "@types/dotenv": "^8.2.0",
    "@types/geojson": "^7946.0.14",
    "@types/jest": "^29.5.12",
    "axios": "^1.6.2",
    "git-commit-msg-linter": "^4.5.0",
    "googleapis": "^129.0.0",
    "husky": "^8.0.0",
    "jest": "^29.6.2",
    "luxon": "^3.4.4",
    "prettier": "3.0.1",
    "ts-jest": "^29.1.2"
  },
  "description": "Welcome to Keystone!",
  "main": "index.js",
  "author": "",
  "license": "ISC"
}
